<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.elmclean.horrorgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartGame" />
        <activity android:name=".TheNorth" />

        <service
            android:name=".MusicService"
            android:enabled="true" />

        <activity android:name=".TheSouth" />
        <activity android:name=".CutRoses" />
        <activity android:name=".House1" />
        <activity android:name=".House2" />
        <activity android:name=".GameOver"></activity>
    </application>

</manifest>